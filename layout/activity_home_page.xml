                app:layout_constraintEnd_toStartOf="@+id/imageView20"
                app:layout_constraintHorizontal_bias="0.568"
                app:layout_constraintStart_toStartOf="@+id/yearfourhome"
                app:layout_constraintTop_toTopOf="@+id/yearfourhome"
